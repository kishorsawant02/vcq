<div class="container">
	<h1>{{title}}</h1>
	<div class="embed-responsive embed-responsive-4by3">
	  {{{data.url}}}
	</div>
	<br>
	<div>
		<h3>{{data.ques_title}}</h3>
	</div>
	<div class="">
		{{#each data.options}}
		    <div class="">
	  			<input type="radio" name="gender" id="{{.}}" value="male" required>
		        <label for="{{.}}">{{.}}</label>
		    </div>
	    {{/each}}
  	</div>
  	<br>
 	<div class="">
  		<input type="button" id="submitPreference" class="btn btn-primary" onclick="submitPreference()" value="Click Me!">
  	</div>
</div>